{
  "name": "ytl-chat-tool",
  "version": "1.0.0",
  "description": "youtube live chat tool",
  "author": "ozelotjp",
  "private": true,
  "scripts": {
    "dev": "webpack",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore .",
    "commit": "npx git-cz"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/webpack": "^4.41.7",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
